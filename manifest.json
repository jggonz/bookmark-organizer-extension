{
  "manifest_version": 3,
  "name": "Bookmark Organizer",
  "version": "0.1.0",
  "description": "Search, tag, and organize bookmarks with AI suggestions.",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "options_page": "options.html",
  "icons": {
    "16": "assets/icon-16.png",
    "48": "assets/icon-48.png",
    "128": "assets/icon-128.png"
  }
}
